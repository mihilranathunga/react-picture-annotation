{
  "name": "@cognite/react-picture-annotation",
  "version": "0.17.0",
  "description": "A simple pdf/image annotation component.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "dist/types/src/index.d.ts",
  "scripts": {
    "build": "tsc --build && rollup -c ./rollup.config.js",
    "storybook": "start-storybook -p 6006  --no-dll",
    "build-storybook": "build-storybook",
    "commit": "npx git-cz",
    "precommit": "yarn lint --fix",
    "eslint": "eslint --fix",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lib:publish": "npm run build && npm publish",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cognitedata/react-picture-annotation.git"
  },
  "keywords": [
    "react",
    "typescript-library",
    "javascript-library",
    "annotation"
  ],
  "author": "Bay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cognitedata/react-picture-annotation/issues"
  },
  "homepage": "https://github.com/cognitedata/react-picture-annotation#readme",
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-async-to-generator": "^7.10.4",
    "@babel/plugin-transform-regenerator": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.8",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@storybook/addon-a11y": "^6.1.0",
    "@storybook/addon-actions": "^6.1.0",
    "@storybook/addon-docs": "^6.1.0",
    "@storybook/addon-essentials": "^6.1.0",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.0",
    "@storybook/addon-storysource": "^6.1.0",
    "@storybook/addons": "^6.0.21",
    "@storybook/react": "^6.1.0",
    "@svgr/rollup": "^5.5.0",
    "@types/jest": "^26.0.14",
    "@types/pdfjs-dist": "^2.1.5",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "^5.1.4",
    "babel-loader": "^8.2.1",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^4.3.0",
    "jest": "^26.5.3",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "react": "^16.14.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "rollup": "^2.33.3",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "semantic-release": "^17.2.3",
    "ts-jest": "^26.4.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0"
  },
  "dependencies": {
    "@cognite/annotations": "^1.0.3",
    "@cognite/cogs.js": "^2.0.3",
    "@cognite/sdk": "^3.2.1",
    "@storybook/addon-knobs": "^6.1.0",
    "@types/lodash": "^4.14.165",
    "parse-color": "^1.0.0",
    "pdf-lib": "^1.12.0",
    "pdfjs-dist": "2.6.347",
    "react-archer": "^3.0.0",
    "react-draggable": "^4.4.3",
    "react-is": "^17.0.1",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.0",
    "typescript": "^4.1.2",
    "uuid": "^8.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "@types/react": "^16.9.35"
  },
  "lint-staged": {
    "{stories,src}/**/*.{ts,tsx}": [
      "tslint --project tsconfig.json -c tslint.json --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
